<h1>お気に入り一覧へのリンク</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>User name</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= link_to user.id, likes_user_path(user) %></td>
          <td><%= user.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  
